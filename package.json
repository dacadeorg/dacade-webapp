{
  "name": "dacade",
  "version": "2.0.0",
  "description": "blockchain education platform",
  "author": "moritzfelipe",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint-fix:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore . --quiet --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint-fix:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint-fix": "npm run lint-fix:js && npm run lint-fix:style",
    "test": "jest",
    "storybook": "nuxt storybook",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --quiet --fix",
    "*.{css,vue}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@coingecko/cryptoformat": "^0.4.4",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/markdownit": "^2.0.0",
    "@nuxtjs/svg": "^0.1.12",
    "@storybook/addon-controls": "^6.1.17",
    "@storybook/addon-notes": "^5.3.21",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.9.1",
    "firebase": "^7.20.0",
    "gray-matter": "^4.0.3",
    "hex-to-rgba": "^2.0.1",
    "highlight.js": "^10.7.2",
    "highlightjs-solidity": "^1.0.22",
    "lodash.chunk": "^4.2.0",
    "markdown-it": "^12.0.6",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-div": "^1.1.0",
    "moment": "^2.29.1",
    "nuxt": "^2.15.3",
    "nuxt-bugsnag": "^4.1.2",
    "nuxt-highlightjs": "^1.0.1",
    "nuxt-i18n": "^6.20.1",
    "v-click-outside": "^3.1.2",
    "vee-validate": "^3.3.11",
    "vue-highlight.js": "^4.0.1-beta",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^3.3.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "@tailwindcss/typography": "^0.4.1",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "fibers": "^5.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
    "standard-version": "^9.3.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "vue-jest": "^3.0.4"
  }
}
